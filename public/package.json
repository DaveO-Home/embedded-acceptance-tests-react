    "map": {
      "babel-polyfill": "babel-polyfill/dist/polyfill"
    },
    "npmAlgorithm": "flat",
    "ext": {
      "css": "steal-css"
    },
    "cssOptions": {
      "timeout": "15"
    },
    "transpiler": "babel",
    "babelOptions": {
      "presets": [
        [
          "es2015",
          "react"
        ]
      ],
      "plugins": [
        "transform-react-jsx"
      ]
    },
    "plugins": [
      "steal-css"
    ],
    "meta": {
      "jquery": {
        "exports": "jQuery",
        "format": "global"
      },
      "bootstrap": {
        "deps": [
          "jquery"
        ]
      },
      "tablesorter": {
        "deps": [
          "jquery",
          "bootstrap"
        ]
      },
      "tablepager": {
        "deps": [
          "jquery",
          "bootstrap"
        ]
      },
      "tablewidgets": {
        "deps": [
          "jquery",
          "bootstrap"
        ]
      },
      "babel-polyfill/dist/polyfill": {
        "format": "cjs"
      }
    }
