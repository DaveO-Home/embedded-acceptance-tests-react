    "b/setglobals": "./browserify/appl/js/utils/set.globals.js",
    "b/setimports": "../browserify/appl/js/utils/set.imports.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify"
    ]
  },
  "browserify-shim": {
    "b/popper": "Popper",
    "bootstrap": {
      "depends": [
        "jquery:jQuery",
        "b/popper:Popper"
      ]
    }
  }
